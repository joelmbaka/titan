{
  "rewrites": [
    {
      "source": "/api/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?<subdomain>[^.]+).joelmbaka.site"
        }
      ],
      "destination": "/api/:path*"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "(?<subdomain>[^.]+).joelmbaka.site"
        }
      ],
      "destination": "/store/:subdomain"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?<subdomain>[^.]+).joelmbaka.site"
        }
      ],
      "destination": "/store/:subdomain/:path*"
    }
  ]
} 